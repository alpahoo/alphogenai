{
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/debug/env",
      "dest": "/api/debug/env"
    },
    {
      "src": "/api/runpod/webhook",
      "dest": "/api/runpod/webhook"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
